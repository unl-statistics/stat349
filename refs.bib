	langid = {english},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/bjep.12309},
	keywords = {academic boredom, emotional crossover, experience sampling, learning motivation, teacher boredom},
	file = {Full Text PDF:/home/susan/Nextcloud/Zotero/storage/NAAUYFLE/Tam et al. - 2020 - Boredom begets boredom An experience sampling study on the impact of teacher boredom on student bor.pdf:application/pdf;PDF:/home/susan/Nextcloud/Zotero/storage/EPPHTDIL/Tam et al. - 2020 - Boredom begets boredom An experience sampling study on the impact of teacher boredom on student bor.pdf:application/pdf;Snapshot:/home/susan/Nextcloud/Zotero/storage/RDNGZMRC/bjep.html:text/html},
